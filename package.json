{
  "version": "0.1.3",
  "license": "MIT",
  "name": "chan-bottom-sheet",
  "author": "Chansu Kim",
  "repository": "https://github.com/ckstn0777/react-bottom-sheet",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "react",
    "modal",
    "bottom-sheet"
  ],
  "main": "dist/index.js",
  "module": "dist/chan-bottom-sheet.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tsdx watch",
    "prepare": "yarn build",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "np",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "np": {
    "yarn": true
  },
  "size-limit": [
    {
      "path": "dist/chan-bottom-sheet.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/chan-bottom-sheet.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/addon-styling": "^1.0.5",
    "@storybook/addon-viewport": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-webpack5": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.23",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.2.4",
    "storybook": "^7.0.7",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  }
}
